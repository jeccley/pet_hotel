<h1>Customer</h1>
<%= link_to "New Customer", new_customer_path %>

<div class="customer">
  <% @customers.each do |customer| %>
    <h2><%= link_to customer.name, customer %></h2>
    <p><%= customer.phone %></p>
    <p><%= mail_to customer.email, customer.email, subject: "Katie's Bunny Boarding: " %></p>
    <p><%= customer.notes %></p>
    <p><%= customer.vet_details %></p>
  <% end %>
</div>
